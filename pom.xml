<project xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://maven.apache.org/POM/4.0.0" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">

  <modelVersion>4.0.0</modelVersion>

  <groupId>soupply</groupId>
  <artifactId>soupply</artifactId>
  <version>2.40.0</version>
  <packaging>pom</packaging>

  <name>soupply</name>
  <description>Automatically generated libraries for encoding and decoding Minecraft protocols</description>

  <modules>
    <module>bedrock</module>
    <module>bedrock137</module>
    <module>bedrock141</module>
    <module>bedrock150</module>
    <module>bedrock160</module>
    <module>bedrock201</module>
    <module>bedrock261</module>
    <module>java</module>
    <module>java210</module>
    <module>java315</module>
    <module>java316</module>
    <module>java335</module>
    <module>java338</module>
    <module>java340</module>
    <module>test0</module>
    <module>util</module>
